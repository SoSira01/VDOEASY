@model ReceiptViewModel

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Receipt</title>
</head>
<body>
    <h1>Receipt</h1>
    <p>Member ID: @Model.MemberId</p>
    <p>Full Name: @Model.FullName</p>
    <p>Address: @Model.Address</p>
    <p>Total Amount: @Model.TotalAmount</p>
</body>
</html>
